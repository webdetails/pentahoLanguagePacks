        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.*;
import java.util.zip.*;
import java.util.List;
import java.util.ArrayList;
static final int BUFFER = 2048;

public void generateFileList( List fileList, File node ) {
	// file
	if ( node.isFile() ) {
		//logBasic( node.getAbsoluteFile().toString() );
		fileList.add( node.getAbsoluteFile().toString() );
	}
	// folder
	if ( node.isDirectory() ) {
		String[] childNodes = node.list();
		for ( int i = 0; i < childNodes.length; i++ ) {
			generateFileList( fileList, new File( node, childNodes[i] ) );
		}
	}
}

public void addToZip( String baseDir, String file, ZipOutputStream out ) {
 	try {
		byte data[] = new byte[BUFFER];
		FileInputStream fis = new FileInputStream(file);
		BufferedInputStream in = new BufferedInputStream(fis, BUFFER);
		out.putNextEntry( new ZipEntry( file.substring( baseDir.length()+1, file.length() ) ) );
		int len;
		while ( (len = in.read(data) ) > 0 ) {
			out.write( data, 0, len );
		}
		out.flush();
		out.closeEntry();
		in.close();
		fis.close();
	} catch( Exception e ) {
		logBasic( "Error adding " + file + " to the zip file." );
	}
} 

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if ( r == null ) {
	  setOutputDone();
	  return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow( r, data.outputRowMeta.size() );
	
    String source = get( Fields.In, "source" ).getString(r);
    String target = get( Fields.In, "target" ).getString(r);
	logBasic( source );
	logBasic( target );
 	try {
		List fileList = new ArrayList();
		generateFileList( fileList, new File( source ) );
		if(fileList.size() > 0){
			FileOutputStream fos = new FileOutputStream( target );
 			ZipOutputStream out = new ZipOutputStream( new BufferedOutputStream( fos ) );
			//out.setMethod( ZipOutputStream.STORED );
        		for ( int i=0; i < fileList.size(); i++ ) {
				String file = (String)fileList.get(i);
				addToZip( source, file, out );
			}
			out.close();
			fos.close();
		} else {
			logBasic( "No files to add to the zip file. ");
		}
	} catch( Exception e ) {
		logBasic( "Error creating the zip file." );
	}

	// Send the row on to the next step.
    putRow( data.outputRowMeta, r );

	return true;
}
]]></class_source>
